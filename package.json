  "scripts": {
    "start": "node scripts/start",
    "build": "node scripts/build",
    "analyze": "node scripts/build --bundle-analyze",
    "test": "node scripts/test",
    "browser-automation": "node scripts/browser-automation",
    "express": "npm run build && node src/server",
    "koa": "npm run build && node src/server koa",
    "hapi": "npm run build && node src/server hapi",
    "lint": "eslint src/* --ext .js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": "eslint:react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test/client",
      "<rootDir>/test/server"
    ],
    "collectCoverageFrom": [
      "client/**/*.{js,jsx,ts,tsx}",
      "!client/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/.enzyme.setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/{client,server}/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/test/{client,server}/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "sinon": "^9.0.1",
    "webpack-bundle-analyzer": "^3.6.1"
  }
